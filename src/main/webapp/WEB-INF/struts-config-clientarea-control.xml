<?xml version="1.0" encoding="UTF-8" ?>
<!--
  aoweb-struts: Do not edit

  This page is the master template for the struts-config-clientarea-control.xml
  file.  If the following files exist, the contents of the file will be
  substituted into the proper place in this file by the build system.

  struts-config-clientarea-control.xml.messageResources Additional message resources
-->
<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
    <!-- ========== Form Bean Definitions =================================== -->
    <form-beans>
        <form-bean name="cancelFeedbackForm" type="com.aoindustries.website.clientarea.control.business.CancelFeedbackForm" />
        <form-bean name="passwordEvaluatorForm" type="com.aoindustries.website.clientarea.control.password.PasswordEvaluatorForm" />
        <form-bean name="businessAdministratorPasswordSetterForm" type="com.aoindustries.website.clientarea.control.password.BusinessAdministratorPasswordSetterForm" />
        <form-bean name="linuxAccountPasswordSetterForm" type="com.aoindustries.website.clientarea.control.password.LinuxAccountPasswordSetterForm" />
        <form-bean name="mySQLPasswordSetterForm" type="com.aoindustries.website.clientarea.control.password.MySQLPasswordSetterForm" />
        <form-bean name="postgreSQLPasswordSetterForm" type="com.aoindustries.website.clientarea.control.password.PostgreSQLPasswordSetterForm" />
        <form-bean name="globalPasswordSetterForm" type="com.aoindustries.website.clientarea.control.password.GlobalPasswordSetterForm" />
    </form-beans>

    <!-- ========== Global Error Handlers =================================== -->
    <global-exceptions>
        <exception key="Exception.message" type="java.lang.Exception" handler="com.aoindustries.website.ExceptionHandler" />
    </global-exceptions>

    <!-- ========== Global Forward Definitions ============================== -->
    <global-forwards>
        <!-- / -->
        <forward name="exception" module="" path="/exception.jsp" redirect="false" />
        <forward name="login" module="" path="/login.do" redirect="false" />
        <forward name="permission-denied" module="" path="/permission-denied.do" redirect="false" />
    </global-forwards>

    <!-- ========== Action Mapping Definitions ============================== -->
    <action-mappings>
        <action
            path="/index"
            type="com.aoindustries.website.SkinAction"
        >
            <forward name="success" path="/index.jsp" redirect="false" />
        </action>
        <action
            path="/business/cancel"
            type="com.aoindustries.website.clientarea.control.business.CancelAction"
        >
            <forward name="permission-denied" path="/business/cancel.jsp" redirect="false" />
            <forward name="success" path="/business/cancel.jsp" redirect="false" />
        </action>
        <action
            path="/business/cancel-feedback"
            name="cancelFeedbackForm"
            scope="request"
            validate="false"
            type="com.aoindustries.website.clientarea.control.business.CancelFeedbackAction"
        >
            <forward name="permission-denied" path="/business/cancel-feedback.jsp" redirect="false" />
            <forward name="invalid-business" path="/business/cancel.do" redirect="true" />
            <forward name="success" path="/business/cancel-feedback.jsp" redirect="false" />
        </action>
        <action
            path="/business/cancel-feedback-completed"
            name="cancelFeedbackForm"
            scope="request"
            validate="false"
            type="com.aoindustries.website.clientarea.control.business.CancelFeedbackCompletedAction"
        >
            <forward name="permission-denied" path="/business/cancel-feedback-completed.jsp" redirect="false" />
            <forward name="invalid-business" path="/business/cancel.do" redirect="true" />
            <forward name="success" path="/business/cancel-feedback-completed.jsp" redirect="false" />
        </action>
        <action
            path="/monitor/mysql-replication-monitor"
            type="com.aoindustries.website.clientarea.control.monitor.MySQLReplicationMonitorAction"
        >
            <forward name="permission-denied" path="/monitor/mysql-replication-monitor.jsp" redirect="false" />
            <forward name="success" path="/monitor/mysql-replication-monitor.jsp" redirect="false" />
        </action>
        <action
            path="/password/index"
            type="com.aoindustries.website.SkinAction"
        >
            <forward name="success" path="/password/index.jsp" redirect="false" />
        </action>
        <action
            path="/password/password-evaluator"
            name="passwordEvaluatorForm"
            scope="request"
            validate="false"
            type="com.aoindustries.website.SkinAction"
        >
            <forward name="success" path="/password/password-evaluator.jsp" redirect="false" />
        </action>
        <action
            path="/password/password-evaluator-completed"
            name="passwordEvaluatorForm"
            scope="request"
            validate="false"
            type="com.aoindustries.website.clientarea.control.password.PasswordEvaluatorCompletedAction"
        >
            <forward name="input" path="/password/password-evaluator.jsp" redirect="false" />
            <forward name="success" path="/password/password-evaluator.jsp" redirect="false" />
        </action>
        <action
            path="/password/password-generator"
            type="com.aoindustries.website.clientarea.control.password.PasswordGeneratorAction"
        >
            <forward name="success" path="/password/password-generator.jsp" redirect="false" />
        </action>
        <action
            path="/password/business-administrator-password-setter"
            name="businessAdministratorPasswordSetterForm"
            scope="request"
            validate="false"
            type="com.aoindustries.website.clientarea.control.password.BusinessAdministratorPasswordSetterAction"
        >
            <forward name="success" path="/password/business-administrator-password-setter.jsp" redirect="false" />
        </action>
        <action
            path="/password/business-administrator-password-setter-completed"
            name="businessAdministratorPasswordSetterForm"
            scope="request"
            validate="false"
            type="com.aoindustries.website.clientarea.control.password.BusinessAdministratorPasswordSetterCompletedAction"
        >
            <forward name="input" path="/password/business-administrator-password-setter.jsp" redirect="false" />
            <forward name="success" path="/password/business-administrator-password-setter.jsp" redirect="false" />
        </action>
        <action
            path="/password/linux-account-password-setter"
            name="linuxAccountPasswordSetterForm"
            scope="request"
            validate="false"
            type="com.aoindustries.website.clientarea.control.password.LinuxAccountPasswordSetterAction"
        >
            <forward name="permission-denied" path="/password/linux-account-password-setter.jsp" redirect="false" />
            <forward name="success" path="/password/linux-account-password-setter.jsp" redirect="false" />
        </action>
        <action
            path="/password/linux-account-password-setter-completed"
            name="linuxAccountPasswordSetterForm"
            scope="request"
            validate="false"
            type="com.aoindustries.website.clientarea.control.password.LinuxAccountPasswordSetterCompletedAction"
        >
            <forward name="permission-denied" path="/password/linux-account-password-setter.jsp" redirect="false" />
            <forward name="input" path="/password/linux-account-password-setter.jsp" redirect="false" />
            <forward name="success" path="/password/linux-account-password-setter.jsp" redirect="false" />
        </action>
        <action
            path="/password/mysql-password-setter"
            name="mySQLPasswordSetterForm"
            scope="request"
            validate="false"
            type="com.aoindustries.website.clientarea.control.password.MySQLPasswordSetterAction"
        >
            <forward name="permission-denied" path="/password/mysql-password-setter.jsp" redirect="false" />
            <forward name="success" path="/password/mysql-password-setter.jsp" redirect="false" />
        </action>
        <action
            path="/password/mysql-password-setter-completed"
            name="mySQLPasswordSetterForm"
            scope="request"
            validate="false"
            type="com.aoindustries.website.clientarea.control.password.MySQLPasswordSetterCompletedAction"
        >
            <forward name="input" path="/password/mysql-password-setter.jsp" redirect="false" />
            <forward name="permission-denied" path="/password/mysql-password-setter.jsp" redirect="false" />
            <forward name="success" path="/password/mysql-password-setter.jsp" redirect="false" />
        </action>
        <action
            path="/password/postgresql-password-setter"
            name="postgreSQLPasswordSetterForm"
            scope="request"
            validate="false"
            type="com.aoindustries.website.clientarea.control.password.PostgreSQLPasswordSetterAction"
        >
            <forward name="permission-denied" path="/password/postgresql-password-setter.jsp" redirect="false" />
            <forward name="success" path="/password/postgresql-password-setter.jsp" redirect="false" />
        </action>
        <action
            path="/password/postgresql-password-setter-completed"
            name="postgreSQLPasswordSetterForm"
            scope="request"
            validate="false"
            type="com.aoindustries.website.clientarea.control.password.PostgreSQLPasswordSetterCompletedAction"
        >
            <forward name="permission-denied" path="/password/postgresql-password-setter.jsp" redirect="false" />
            <forward name="input" path="/password/postgresql-password-setter.jsp" redirect="false" />
            <forward name="success" path="/password/postgresql-password-setter.jsp" redirect="false" />
        </action>
        <action
            path="/password/global-password-setter"
            name="globalPasswordSetterForm"
            scope="request"
            validate="false"
            type="com.aoindustries.website.clientarea.control.password.GlobalPasswordSetterAction"
        >
            <forward name="permission-denied" path="/password/global-password-setter.jsp" redirect="false" />
            <forward name="success" path="/password/global-password-setter.jsp" redirect="false" />
        </action>
        <action
            path="/password/global-password-setter-completed"
            name="globalPasswordSetterForm"
            scope="request"
            validate="false"
            type="com.aoindustries.website.clientarea.control.password.GlobalPasswordSetterCompletedAction"
        >
            <forward name="permission-denied" path="/password/global-password-setter.jsp" redirect="false" />
            <forward name="input" path="/password/global-password-setter.jsp" redirect="false" />
            <forward name="success" path="/password/global-password-setter.jsp" redirect="false" />
        </action>
        <action
            path="/vnc/vnc-console"
            type="com.aoindustries.website.clientarea.control.vnc.VncConsoleAction"
        >
            <forward name="permission-denied" path="/vnc/vnc-console.jsp" redirect="false" />
            <forward name="success" path="/vnc/vnc-console.jsp" redirect="false" />
        </action>
        <action
            path="/vnc/vnc-viewer"
            type="com.aoindustries.website.clientarea.control.vnc.VncViewerAction"
        >
            <forward name="success" path="/vnc/vnc-viewer.jsp" redirect="false" />
        </action>
        <action
            path="/**"
            type="com.aoindustries.website.SkinAction"
        >
            <forward name="success" module="" path="/not-found.jsp" redirect="false" />
        </action>
    </action-mappings>
    
    <!-- ========== Controller ============================================== -->
    <controller
        nocache="true"
    />

    <!-- ========== Message Resources ======================================= -->
    <message-resources
        factory="com.aoindustries.website.struts.ResourceBundleMessageResourcesFactory"
        parameter="com.aoindustries.website.SiteApplicationResources"
        null="false"
    />
    <message-resources
        key="/ApplicationResources"
        factory="com.aoindustries.website.struts.ResourceBundleMessageResourcesFactory"
        parameter="com.aoindustries.website.ApplicationResources"
        null="false"
    />
    <message-resources
        key="/clientarea/ApplicationResources"
        factory="com.aoindustries.website.struts.ResourceBundleMessageResourcesFactory"
        parameter="com.aoindustries.website.clientarea.ApplicationResources"
        null="false"
    />
    <message-resources
        key="/clientarea/accounting/ApplicationResources"
        factory="com.aoindustries.website.struts.ResourceBundleMessageResourcesFactory"
        parameter="com.aoindustries.website.clientarea.accounting.ApplicationResources"
        null="false"
    />
    <message-resources
        key="/clientarea/control/ApplicationResources"
        factory="com.aoindustries.website.struts.ResourceBundleMessageResourcesFactory"
        parameter="com.aoindustries.website.clientarea.control.ApplicationResources"
        null="false"
    />
    <message-resources
        key="/clientarea/ticket/ApplicationResources"
        factory="com.aoindustries.website.struts.ResourceBundleMessageResourcesFactory"
        parameter="com.aoindustries.website.clientarea.ticket.ApplicationResources"
        null="false"
    />
    <message-resources
        key="/clientarea/webmail/ApplicationResources"
        factory="com.aoindustries.website.struts.ResourceBundleMessageResourcesFactory"
        parameter="com.aoindustries.website.clientarea.webmail.ApplicationResources"
        null="false"
    />

<!-- Include "struts-config-clientarea-control.xml.messageResources" -->

    <!-- ========== Validator Plug-In ======================================= -->
    <plug-in className="org.apache.struts.validator.ValidatorPlugIn">
        <set-property property="pathnames" value="/org/apache/struts/validator/validator-rules.xml,/WEB-INF/validator-custom-rules.xml,/WEB-INF/validator-constants.xml,/WEB-INF/validator-clientarea-control.xml" />
    </plug-in>
</struts-config>
